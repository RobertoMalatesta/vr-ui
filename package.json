{
  "name": "vr-ui",
  "version": "0.0.10",
  "description": "simple Three.js based library to create VR UI.",
  "repository": "git@github.com:artflow-vr/vr-ui.git",
  "bugs": "https://github.com/artflow-vr/vr-ui/issues",
  "author": "David Peicho <david.peicho@epita.fr>",
  "license": "MIT",
  "keywords": [
    "vr",
    "ui",
    "threejs",
    "3D"
  ],
  "main": "build/vr-ui.min.js",
  "module": "vr-ui.js",
  "files": [
    "package.json",
    "LICENSE",
    "README.md",
    "build/vr-ui.min.js",
    "src",
    "examples"
  ],
  "scripts": {
    "build": "webpack --progress --config webpack.config.js --config-name library",
    "dev": "webpack-dev-server --hot --inline",
    "lint": "eslint src",
    "test": "webpack --progress --config webpack.config.js --config-name test && node ./test/run.js",
    "prepublish": "yarn run lint && yarn run test && yarn run build"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "dotenv": "^4.0.0",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.9.0",
    "mocha": "^5.0.0",
    "path": "^0.12.7",
    "three": "^0.89.0",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "parse-bmfont-ascii": "^1.0.6",
    "three-bmfont-text": "^2.2.1"
  }
}
